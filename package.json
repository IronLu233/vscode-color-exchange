        "@types/glob": "^5.0.30",
        "decache": "^4.1.0",
        "glob": "^7.1.1",
        "istanbul": "^0.4.5",
        "mocha": "^3.2.0",
        "remap-istanbul": "^0.8.4",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
