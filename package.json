{
    "name": "vscode-color-exchange",
    "displayName": "color-exchange",
    "description": "Exchange hexadecimal color, rgb color, hsl color etc.",
    "version": "1.1.0",
    "publisher": "IronLu233",
    "keywords": [
        "color",
        "css",
        "refactor"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/IronLu233/vscode-color-exchange.git"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Formatters",
        "Snippets"
    ],
    "icon": "favicon.png",
